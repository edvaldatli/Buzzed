{
  "expo": {
    "name": "Buzzed",
    "description": "A social media app for the modern age",
    "sdkVersion": "52.0.0",
    "slug": "Buzzed",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#FF80C6",
    "splash": {
      "image": "./assets/images/BuzzedSplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF80C6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.FrostFallStudios.Buzzed"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#FF80C6"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.FrostFallStudios.Buzzed"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appstore.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Rubik/Rubik-Black.ttf",
            "assets/fonts/Rubik/Rubik-BlackItalic.ttf",
            "assets/fonts/Rubik/Rubik-Bold.ttf",
            "assets/fonts/Rubik/Rubik-BoldItalic.ttf",
            "assets/fonts/Rubik/Rubik-Italic.ttf",
            "assets/fonts/Rubik/Rubik-Light.ttf",
            "assets/fonts/Rubik/Rubik-LightItalic.ttf",
            "assets/fonts/Rubik/Rubik-Medium.ttf",
            "assets/fonts/Rubik/Rubik-MediumItalic.ttf",
            "assets/fonts/Rubik/Rubik-Regular.ttf",
            "assets/fonts/Rubik/Rubik-SemiBold.ttf",
            "assets/fonts/Rubik/Rubik-SemiBoldItalic.ttf",
            "assets/fonts/Rubik/Rubik-Light.ttf",
            "assets/fonts/Rubik/Rubik-LightItalic.ttf",
            "assets/fonts/Signika/SignikaNegative-Bold.ttf",
            "assets/fonts/Signika/SignikaNegative-Light.ttf",
            "assets/fonts/Signika/SignikaNegative-Regular.ttf",
            "assets/fonts/Signika/SignikaNegative-SemiBold.ttf",
            "assets/fonts/Signika/SignikaNegative-Medium.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Buzzed to access your camera. We use the camera to take a selfie so your friends can see you!"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "99a5d268-4e11-45cf-b179-cee7baab9240"
      }
    }
  }
}
